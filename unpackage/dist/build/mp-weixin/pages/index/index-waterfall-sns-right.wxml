<block wx:for="{{rightList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="feed-one"><navigator open-type="navigate" url="{{'/subpages/feedInfo?id='+item.id}}"><image class="feed-img" src="{{item.cover}}" mode="widthFix" lazy-load="{{true}}"></image><block wx:if="{{!!item.feed_content}}"><view class="u-line-2 feed-title">{{''+item.feed_content+''}}</view></block><view class="feed-info"><view class="iview"><image class="avatar" src="{{item.avatar}}"></image><text class="name u-line-1">{{item.name}}</text></view><view class="iview"><view data-event-opts="{{[['tap',[['clickLove',['$0'],[[['rightList','',index]]]]]]]}}" class="ilike" catchtap="__e"><block wx:if="{{item.has_like}}"><image class="micon" src="/static/lover.png"></image></block><block wx:else><image class="micon" src="/static/love.png"></image></block><block wx:if="{{item.like_count>0}}"><text class="love-count">{{item.like_count}}</text></block></view></view></view></navigator></view></block>